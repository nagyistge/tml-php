        "translations": [
            {
                "label": "TML for PHP\u662f\u4e00\u4e2a\u5ba2\u6237\u7aefSDK\u5e93\uff0c\u5141\u8bb8PHP\u5e94\u7528\u7a0b\u5e8f\u4e0eTranslationExchange\u672c\u5730\u5316\u5e73\u53f0\u96c6\u6210\u3002"
            }
        ]
    },
    "82076047d3de0349c0d9cf0e157c4c19": {
        "original": {
            "label": "Welcome to Tml For PHP"
        },
        "translations": [
            {
                "label": "\u6b22\u8fce\u6765\u5230TML\u5bf9\u4e8ePHP"
            }
        ]
    },
    "24b0c52fd011571988efe66d68d7598f": {
