{
    "2f5894e8b9f7a6063b2a7db9d77c3fae": {
        "original": {
            "label": "The Tml service pulls down new translations from not only your users but users of other websites as you sync your translations."
        },
        "translations": [
            {
                "label": "\u062e\u062f\u0645\u0629 TML \u064a\u0633\u062d\u0628 \u0625\u0644\u0649 \u0623\u0633\u0641\u0644 \u062a\u0631\u062c\u0645\u0627\u062a \u062c\u062f\u064a\u062f\u0629 \u0645\u0646 \u0627\u0644\u062e\u0627\u0635 \u0628\u0643 \u0644\u064a\u0633 \u0641\u0642\u0637 \u0644\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0648\u0644\u0643\u0646 \u0645\u0633\u062a\u062e\u062f\u0645\u064a \u0645\u0648\u0627\u0642\u0639 \u0623\u062e\u0631\u0649 \u0643\u0645\u0627 \u0643\u0646\u062a \u0645\u0632\u0627\u0645\u0646\u0629 \u062a\u0631\u062c\u0645\u0627\u062a\u0643."
            }
        ]
    },
    "da3894f60c9ddef363b6401dfa7e7c92": {
        "original": {
            "label": "Translations from your site get synced to Tml service and can then be shared with thousands of other websites."
        },
        "translations": [
            {
                "label": "\u062a\u0631\u062c\u0645\u0629 \u0645\u0646 \u0645\u0648\u0642\u0639 \u0627\u0644\u0648\u064a\u0628 \u0627\u0644\u062e\u0627\u0635 \u0628\u0643 \u0627\u0644\u062d\u0635\u0648\u0644 \u0639\u0644\u0649 \u0645\u0632\u0627\u0645\u0646\u0629 \u0644\u062e\u062f\u0645\u0629 TML \u0648\u064a\u0645\u0643\u0646 \u0628\u0639\u062f \u0630\u0644\u0643 \u062a\u0642\u0627\u0633\u0645\u0647\u0627 \u0645\u0639 \u0627\u0644\u0622\u0644\u0627\u0641 \u0645\u0646 \u0627\u0644\u0645\u0648\u0627\u0642\u0639 \u0627\u0644\u0623\u062e\u0631\u0649."
            }
        ]
    },
    "50303972e76949144671e338b405983d": {
        "original": {
            "label": "Your multilingual users can sign up to help translate your site into over {count || different language}."
        },
        "translations": [
            {
                "label": "\u064a\u0645\u0643\u0646 \u0644\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0627\u0644\u0627\u0634\u062a\u0631\u0627\u0643 \u0645\u062a\u0639\u062f\u062f \u0627\u0644\u0644\u063a\u0627\u062a \u0644\u0644\u0645\u0633\u0627\u0639\u062f\u0629 \u0641\u064a \u062a\u0631\u062c\u0645\u0629 \u0645\u0648\u0642\u0639\u0643 \u0625\u0644\u0649 \u0623\u0643\u062b\u0631 \u0645\u0646 {count} \u0644\u063a\u0627\u062a \u0645\u062e\u062a\u0644\u0641\u0629."
            }
        ]
    },
    "b45bd948e3a7ad068c511d4e40e2187b": {
