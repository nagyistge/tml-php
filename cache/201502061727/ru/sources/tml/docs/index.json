    "4e12cb1c1c68c791c81b98f9ed7ec4c6": {
        "original": {
            "label": " To initialize the SDK, call the initialization function with the key and secret of your application: "
        },
        "translations": [
            {
                "label": "\u0414\u043b\u044f \u0438\u043d\u0438\u0446\u0438\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438 SDK, \u0432\u044b\u0437\u043e\u0432\u0438\u0442\u0435 \u0444\u0443\u043d\u043a\u0446\u0438\u044e \u0438\u043d\u0438\u0446\u0438\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438 \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043a\u043b\u044e\u0447\u0430 \u0438 \u0442\u0430\u0439\u043d\u044b \u0432\u0430\u0448\u0435\u0433\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f:"
            }
        ]
    },
    "97190faac43a951fc4bae7cc89ea607d": {
